  "engines": {
    "node": ">=0.10.0"
  },
  "keywords": [
    "yui",
    "jsdoc",
    "api",
    "documentation",
    "javadoc",
    "docs",
    "apidocs"
  ],
  "main": "./lib/index",
  "bin": {
    "firedoc": "./lib/cli.js",
    "firedoc-theme": "./lib/cli-theme.js"
  },
  "dependencies": {
    "express": "^4.12.3",
    "graceful-fs": "2.x",
    "markdown-it": "^4.1.0",
    "minimatch": "^2.0.1",
    "minimist": "^1.1.1",
    "rimraf": "2.x",
    "yui": "^3.18.1"
  },
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jshint": "^2.6.0",
    "selleck": "^0.1.18",
    "ytestrunner": "^0.3.3",
    "yuitest": "^0.7.9"
  },
  "scripts": {
    "docs": "make docs",
    "pretest": "jshint ./lib/*.js ./tests/*.js",
    "test": "istanbul cover --print=both --yui ytestrunner -- --include ./tests/options.js --include ./tests/builder.js --include ./tests/parser.js --include ./tests/parser_coffee.js --include ./tests/files.js --include ./tests/utils.js --include ./tests/preprocessor.js"
  },
  "preferGlobal": "true",
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/fireball-x/firedoc/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/fireball-x/firedoc.git"
  },
  "jshintConfig": {
    "bitwise": true,
    "browser": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "latedef": "nofunc",
    "laxbreak": true,
    "maxerr": 500,
    "maxlen": 150,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "onevar": true,
    "trailing": true,
    "undef": true,
    "unused": "vars",
    "yui": true
  },
  "yuidoc": {
    "name": "FireDoc",
    "options": {
      "linkNatives": "true",
      "attributesEmit": "true",
      "paths": [
        "./lib"
      ],
      "outdir": "./docs"
    }
  }
}
